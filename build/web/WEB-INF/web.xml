<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginControllerServlet</servlet-name>
        <servlet-class>ServletPackage.LoginControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpControllerServlet</servlet-name>
        <servlet-class>ServletPackage.SignUpControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SupplierAddRestuarantControllerServlet</servlet-name>
        <servlet-class>ServletPackage.SupplierAddRestuarantControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDishControllerServlet</servlet-name>
        <servlet-class>ServletPackage.AddDishControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDishControllerServlet</servlet-name>
        <servlet-class>ServletPackage.DeleteDishControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminDeleteUserControllerServlet</servlet-name>
        <servlet-class>ServletPackage.AdminDeleteUserControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminVerifyRestuarantControllerServlet</servlet-name>
        <servlet-class>ServletPackage.AdminVerifyRestuarantControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerAddToCartControllerServlet</servlet-name>
        <servlet-class>ServletPackage.CustomerAddToCartControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerOrderReceivedServlet</servlet-name>
        <servlet-class>ServletPackage.CustomerOrderReceivedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderShippedControllerServlet</servlet-name>
        <servlet-class>ServletPackage.OrderShippedControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginControllerServlet</servlet-name>
        <url-pattern>/LoginControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpControllerServlet</servlet-name>
        <url-pattern>/SignUpControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SupplierAddRestuarantControllerServlet</servlet-name>
        <url-pattern>/SupplierAddRestuarantControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDishControllerServlet</servlet-name>
        <url-pattern>/AddDishControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDishControllerServlet</servlet-name>
        <url-pattern>/DeleteDishControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminDeleteUserControllerServlet</servlet-name>
        <url-pattern>/AdminDeleteUserControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminVerifyRestuarantControllerServlet</servlet-name>
        <url-pattern>/AdminVerifyRestuarantControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerAddToCartControllerServlet</servlet-name>
        <url-pattern>/CustomerAddToCartControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerOrderReceivedServlet</servlet-name>
        <url-pattern>/CustomerOrderReceivedServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderShippedControllerServlet</servlet-name>
        <url-pattern>/OrderShippedControllerServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
